<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="person1" class="com.prowings.models.Person">
		<property name="id" value="123" />
		<property name="name" value="Ram" />
		<property name="mobileNumbers">
			<list>
				<value>98765</value>
				<value>67890</value>
				<value>12344</value>
			</list>
		</property>
		<property name="emailIds">
			<set>
				<value>abc@gmail.com</value>
				<value>pqr@prowings.com</value>
				<value>xyz@yahoo.com</value>
				<value>pqr@prowings.com</value>
			</set>
		</property>
	</bean>

	<bean id="car2" class="com.prowings.models.Car">
		<constructor-arg value="20" type="int" />
		<constructor-arg value="Elantra" />
	</bean>


</beans>